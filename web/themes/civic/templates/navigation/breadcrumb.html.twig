{#
/**
 * @file
 * Theme override for a breadcrumb trail.
 *
 * Available variables:
 * - breadcrumb: Breadcrumb trail items.
 */
#}
{% if breadcrumb %}
  {% for item in breadcrumb %}
    {% if item.url %}
      <a class="breadcrumb-item" href="{{ item.url }}">{{ item.text }}</a> 
    {% else %}
      <span class="breadcrumb-item">{{ item.text }}</span>
    {% endif %}
  {% endfor %}
{% endif %}
